!map escape to caps lock
remove lock = Caps_Lock
keycode 66 = Escape

remove mod1 = Alt_L
remove mod4 = Super_L
keycode 133 = Alt_L
keycode 64 = Super_L
add mod1 = Alt_L
add mod4 = Super_L

remove mod1 = Alt_R
keycode 108 = Super_R
add mod4 = Super_R

keycode 135 = Control_R
keycode 105 = Menu
add control = Control_R

